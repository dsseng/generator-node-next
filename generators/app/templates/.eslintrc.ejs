{
  <% if (unit && runner == "jest") { %>"plugins": ["jest"],<% } %>
  "rules": {
    <% if (unit && runner == "jest") { %>"jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/prefer-to-have-length": "warn",
    "jest/valid-expect": "error",<% } %>
    "strict": 0
  },
  "parser": "babel-eslint"
}
